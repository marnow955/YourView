    <Button mnemonicParsing="false">
        <graphic>
            <Glyph fontFamily="FontAwesome" icon="expand"/>
        </graphic>
    </Button>
    <Button mnemonicParsing="false">
        <graphic>
            <Glyph fontFamily="FontAwesome" icon="compress"/>
        </graphic>
    </Button>
    <Separator/>
    <Button mnemonicParsing="false">
        <graphic>
            <Glyph fontFamily="FontAwesome" icon="repeat"/>
        </graphic>
    </Button>
    <Button mnemonicParsing="false">
        <graphic>
            <Glyph fontFamily="FontAwesome" icon="undo"/>
        </graphic>
    </Button>
</ToolBar>
